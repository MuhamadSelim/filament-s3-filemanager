{
    "name": "muhamad-selim/filament-s3-filemanager",
    "description": "A Filament v3 plugin for S3 file management with file and folder selection support",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Muhamad Selim",
            "email": "muhamad.selim@example.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "filament/filament": "^3.2",
        "laravel/framework": "^11.0",
        "league/flysystem": "^3.0",
        "league/flysystem-aws-s3-v3": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "MuhamadSelim\\FilamentS3Filemanager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MuhamadSelim\\FilamentS3Filemanager\\FilamentS3FilemanagerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

